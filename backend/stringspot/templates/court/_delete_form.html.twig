<form method="post" action="{{ path('app_back_education_delete', { id: education.id }) }}" onsubmit="return confirm('Are you sure you want to delete this education?');">
  <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ education.id) }}" />
  <button class="btn btn-danger btn-sm mx-1 mb-2 bi bi-trash">Delete</button>
</form>
