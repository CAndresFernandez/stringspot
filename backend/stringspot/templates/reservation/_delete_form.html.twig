<form method="post" action="{{ path('app_back_tech_delete', { id: tech.id }) }}" onsubmit="return confirm('Are you sure you want to delete this tech?');">
  <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ tech.id) }}" />
  <button class="btn btn-danger btn-sm mx-1 mb-2 bi bi-trash">Delete</button>
</form>
